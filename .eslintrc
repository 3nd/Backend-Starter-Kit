{
  "plugins": [],
  "env": {
    "node": true,
    "commonjs": true,
    "es6": true,
    "jest": true
  },
  "globals": {},
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "settings": {
    "import/resolver": {
      "babel-plugin-root-import": {
        "rootPathPrefix": "~",
        "rootPathSuffix": "src"
      }
    }
  },
  "rules": {
    // Plugins
    "import/prefer-default-export": "off",

    // Possible Errors
    "no-console": "off",

    // Best Practices

    // Variables

    // no-restricted-globals

    // Node.js and CommonJS

    // Stylistic Issues
    "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }],
    "no-underscore-dangle": "off"

    // JS.Next
  }
}
