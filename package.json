{
  "name": "Express-Starter-Kit",
  "version": "1.0.0",
  "description": "A boilerplate written in Pug, Stylus, and TypeScript using Express, Material, Heroku, mLab, Gulp, and Rollup.",
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"gulp\"",
    "test": "tsc test/empty.ts && mocha",
    "prod": "tsc && pm2 start src/app.js -i 0 --name \"app\"",
    "reset": "git reset --hard HEAD && git clean -df",
    "reinstall": "yarn cache clean && git clean -dfx -e '.*' && yarn install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Express-Starter-Kit.git"
  },
  "keywords": [
    "Express"
  ],
  "author": "陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Express-Starter-Kit/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Express-Starter-Kit#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "connect-timeout": "^1.7.0",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-alpha.1",
    "csurf": "^1.9.0",
    "errorhandler": "^1.4.3",
    "express": "^5.0.0-alpha.2",
    "express-session": "^1.14.1",
    "material-design-lite": "^1.2.1",
    "method-override": "^2.3.6",
    "mongoose": "^4.6.1",
    "morgan": "^1.7.0",
    "pm2": "^2.0.19",
    "pug": "^2.0.0-beta6",
    "response-time": "^2.3.1",
    "serve-favicon": "^2.3.0",
    "serve-index": "^1.8.0",
    "stylus": "^0.54.5",
    "typescript": "^2.0.2",
    "vhost": "^3.0.2"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.32",
    "@types/browser-sync": "0.0.32",
    "@types/cookie-parser": "^1.3.29",
    "@types/core-js": "^0.9.34",
    "@types/errorhandler": "0.0.29",
    "@types/express": "^4.0.32",
    "@types/gulp": "^3.8.31",
    "@types/gulp-nodemon": "0.0.28",
    "@types/mongoose": "^4.5.35",
    "@types/morgan": "^1.7.31",
    "@types/node": "^6.0.38",
    "browser-sync": "^2.14.3",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "concurrently": "^2.2.0",
    "gulp-changed": "^1.3.2",
    "gulp-cssnano": "^2.1.2",
    "gulp-nodemon": "^2.1.0",
    "gulp-stylus": "^2.5.0",
    "gulp-typescript": "^3.0.2",
    "merge2": "^1.0.2",
    "mocha": "^3.0.2",
    "poststylus": "^0.2.3",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-stream": "^1.14.0",
    "rucksack-css": "^0.8.6",
    "ts-node": "^1.3.0",
