{
  "name": "Express4TS-Quick-Start",
  "version": "1.0.0",
  "description": "Getting started with Express 4 using TypeScript",
  "scripts": {
    "start": "npm run tsc && concurrently \"npm run tsc:w\" \"npm run nodemon\"",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "nodemon": "nodemon src/app.js",
    "reset": "git reset --hard HEAD && git clean -df",
    "postinstall": "npm run typings -- install",
    "typings": "typings"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Express4TS-Quick-Start.git"
  },
  "keywords": [
    "Express 4",
    "TypeScript"
  ],
  "author": "Shyam Chen, 陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Express4TS-Quick-Start/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Express4TS-Quick-Start#readme",
  "dependencies": {
    "express": "^4.14.0",
    "open": "0.0.5",
    "pug": "^2.0.0-beta4"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "nodemon": "^1.10.0",
    "typescript": "^1.8.10",
    "typings": "^1.3.2"
  }
}
