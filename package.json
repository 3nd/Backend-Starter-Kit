{
  "name": "Backend-Starter-Kit",
  "version": "1.0.0",
  "description": "...",
  "scripts": {
    "start": "concurrently \"nodemon src/index.js --exec babel-node\" \"gulp\"",
    "test": "mocha --compilers js:babel-register",
    "build": "babel src -d dist",
    "serve": "node dist/index.js",
    "prod": "pm2 start dist/index.js -i 0 --name \"app\"",
    "reset": "git reset --hard HEAD && git clean -df",
    "reinstall": "yarn cache clean && git clean -dfx -e '.*' && yarn install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Backend-Starter-Kit.git"
  },
  "keywords": [
    "Node"
  ],
  "author": "陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Backend-Starter-Kit/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Backend-Starter-Kit#readme",
  "babel": {
    "presets": [
      "latest"
    ]
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "connect-timeout": "^1.7.0",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-alpha.1",
    "csurf": "^1.9.0",
    "errorhandler": "^1.4.3",
    "express": "^5.0.0-alpha.2",
    "express-session": "^1.14.1",
    "method-override": "^2.3.6",
    "mongoose": "^4.6.1",
    "morgan": "^1.7.0",
    "pm2": "^2.0.19",
    "response-time": "^2.3.1",
    "serve-favicon": "^2.3.0",
    "serve-index": "^1.8.0",
    "vhost": "^3.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-preset-latest": "^6.16.0",
    "browser-sync": "^2.14.3",
    "chai": "^3.5.0",
    "concurrently": "^2.2.0",
    "gulp-changed": "^1.3.2",
    "gulp-nodemon": "^2.1.0",
    "lite-server": "^2.2.2",
    "merge2": "^1.0.2",
    "mocha": "^3.0.2",
    "run-sequence": "^1.2.2",
    "sinon": "^1.17.6"
  }
}
