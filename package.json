{
  "name": "Backend-Starter-Kit",
  "version": "1.0.0",
  "description": "A boilerplate for Node.js, Express, Mongoose, Heroku, mLab, Nodemon, PM2, and Babel.",
  "scripts": {
    "start": "nodemon src/index.js --exec babel-node",
    "dev": "nodemon src/index.js --exec babel-node",
    "test": "mocha --compilers js:babel-register",
    "prod": "babel src -d dist && pm2 start dist/index.js -i 0 --name \"app\"",
    "clean": "rimraf dist",
    "reset": "git reset --hard HEAD && git clean -df",
    "reinstall": "yarn cache clean && git clean -dfx -e '.*' && yarn install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Backend-Starter-Kit.git"
  },
  "keywords": [
    "Node.js"
  ],
  "author": "Yen-cheng Chen, 陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Backend-Starter-Kit/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Backend-Starter-Kit#readme",
  "babel": {
    "presets": [
      "latest"
    ],
    "plugins": [
      "transform-function-bind"
    ]
  },
  "dependencies": {
    "babel-cli": "6.23.0",
    "babel-plugin-transform-function-bind": "6.22.0",
    "babel-preset-latest": "6.22.0",
    "body-parser": "1.16.1",
    "compression": "1.6.2",
    "d3": "4.6.0",
    "express": "4.14.1",
    "express-jwt": "5.1.0",
    "immutable": "3.8.1",
    "jsdom": "9.11.0",
    "lodash": "4.17.4",
    "mongoose": "4.8.5",
    "morgan": "1.8.1",
    "pm2": "2.4.0",
    "prerender-node": "2.7.0",
    "redux": "3.6.0",
    "redux-observable": "0.13.0",
    "rxjs": "5.2.0"
  },
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "babel-register": "6.23.0",
    "chai": "3.5.0",
    "eslint": "3.16.1",
    "mocha": "3.2.0",
    "nodemon": "1.11.0",
    "rimraf": "2.6.1",
    "sinon": "1.17.7"
  }
}
