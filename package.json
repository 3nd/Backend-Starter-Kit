{
  "name": "Express5TS-Quick-Start",
  "version": "1.0.0",
  "description": "Getting started with Express 5 using TypeScript",
  "scripts": {
    "start": "npm run tsc && concurrently \"npm run tsc:w\" \"gulp\"",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "reset": "git reset --hard HEAD && git clean -df",
    "reinstall": "npm cache clean && git clean -dfx -e '.*' && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Express5TS-Quick-Start.git"
  },
  "keywords": [
    "Express 5",
    "TypeScript"
  ],
  "author": "Shyam Chen, 陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Express5TS-Quick-Start/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Express5TS-Quick-Start#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "connect-timeout": "^1.7.0",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-alpha.1",
    "csurf": "^1.9.0",
    "errorhandler": "^1.4.3",
    "express": "^5.0.0-alpha.2",
    "express-session": "^1.14.1",
    "method-override": "^2.3.6",
    "mongoose": "^4.6.0",
    "morgan": "^1.7.0",
    "pug": "^2.0.0-beta4",
    "response-time": "^2.3.1",
    "serve-favicon": "^2.3.0",
    "serve-index": "^1.8.0",
    "stylus": "^0.54.5",
    "typescript": "^2.0.2",
    "vhost": "^3.0.2"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.32",
    "@types/browser-sync": "0.0.32",
    "@types/express": "^4.0.32",
    "@types/gulp": "^3.8.31",
    "@types/gulp-nodemon": "0.0.28",
    "@types/morgan": "^1.7.31",
    "@types/node": "^6.0.38",
    "browser-sync": "^2.14.3",
    "concurrently": "^2.2.0",
    "gulp-nodemon": "^2.1.0",
    "ts-node": "^1.3.0"
  }
}
